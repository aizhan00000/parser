#!/Users/aijanmoldogazieva/Desktop/neo_bootcamp/week2/lections/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'be==0.3.8','console_scripts','be'
__requires__ = 'be==0.3.8'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('be==0.3.8', 'console_scripts', 'be')()
    )
